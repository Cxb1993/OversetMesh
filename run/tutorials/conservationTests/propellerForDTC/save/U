FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (-0.386892 0 0);

boundaryField
{
    overset
    {
        type             overset;
        coupledFringe    yes;
        setHoleCellValue yes;
        holeCellValue    (0 0 0);
        value            nonuniform 0();
    }
    shaft
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    shaftLong
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    propeller
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    propellerFarfield
    {
        type            emptyOverset;
        value           uniform (0 0 0);
    }
    propellerHole
    {
        type            emptyOverset;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (-0.386892 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (-0.386892 0 0);
        value           uniform (-0.386892 0 0);
    }
    farfield
    {
        type            inletOutlet;
        inletValue      uniform (-0.386892 0 0);
        value           uniform (-0.386892 0 0);
    }
}


