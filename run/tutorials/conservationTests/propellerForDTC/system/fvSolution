// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver            BiCGStab;
        preconditioner    Cholesky;
        minIter           0;
        maxIter           1000;
        tolerance         1e-14; // Tight tolerance for conservation testing
        relTol            0.0;
    }
}

PISO
{
    pRefPoint (-1.35 -0.65 0);
    pRefValue 0;

    nNonOrthogonalCorrectors 2;
    nCorrectors 0;
}


relaxationFactors
{
    fields
    {
        p           1;
    }
}


