FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10000;

deltaT          1;

writeControl    runTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
(
    forces
    {
        type forceCoeffs;
        functionObjectLibs ("libforces.so");
        patches (solidWall);

        pName p;
        UName U;
        rhoName rhoInf;
        rhoInf 1;

        outputControl timeStep;
        outputInterval 1;
        log	true;

        CofR (0.25 0 0);
        liftDir (-0.239719744 0.970842132 0);
        dragDir (0.970842132 0.239719744 0);
        magUInf 27.13;
        lRef	0.901;
        Aref	0.901;
        pitchAxis (0 1 0);
    }
);

// ************************************************************************* //
